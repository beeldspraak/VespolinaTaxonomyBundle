<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="vespolina_taxonomy.taxonomy_manager.class">Vespolina\TaxonomyBundle\Document\TaxonomyManager</parameter>
        <parameter key="vespolina_taxonomy.model.taxonomy.class">Vespolina\TaxonomyBundle\Document\Taxonomy</parameter>
    </parameters>

    <services>
        <service id="vespolina_taxonomy.taxonomy_manager" class="%vespolina_taxonomy.taxonomy_manager.class%">
            <argument type="service" id="service_container" />
        </service>
        <service id="vespolina_taxonomy.order_manager" alias="vespolina_taxonomy.taxonomy_manager" />
    </services>
</container>
